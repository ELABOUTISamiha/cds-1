    "@angular/compiler-cli": "7.2.14",
    "@compodoc/compodoc": "1.1.9",
    "@sentry/cli": "1.41.2",
    "@types/d3": "5.7.2",
    "@types/dagre": "0.7.41",
    "@types/jasmine": "3.3.12",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.123",
    "@types/node": "11.13.8",
    "codelyzer": "5.0.1",
    "copy-webpack-plugin": "5.0.3",
    "husky": "2.1.0",
